    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        android:layout_marginTop="12dp"
        android:text="Receipt sample"
        android:textSize="18sp"
        android:textColor="#888"  />

    <LinearLayout
        android:orientation="horizontal"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/btnPrintChineseMenu"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:background="@drawable/common_button"
            android:text="中文"
            android:textSize="@dimen/common_button_text_size"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btnPrintEnglishMenu"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/common_button"
            android:text="English"
            android:textSize="@dimen/common_button_text_size"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btnPrintKoreanMenu"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/common_button"
            android:text="한국어"
            android:textSize="@dimen/common_button_text_size"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/btnPrintJapaneseMenu"
            android:layout_width="110dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/common_button"
            android:text="日本語"
            android:textSize="@dimen/common_button_text_size"
            android:textColor="@color/white" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="Advanced Demo"
            android:textSize="22sp"
            android:textColor="@color/title_text"
            />
        <ToggleButton
            android:layout_width="40dp"
            android:layout_height="34dp"
            android:layout_marginTop="12dp"
            android:checked="true"
            android:textOff=""
            android:textOn=""
            android:background="@drawable/expand"
            android:id="@+id/expand"
            />
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        android:id="@+id/window">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:text="@string/Custom_text"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="#6a6a6a" />

            <Spinner
                android:id="@+id/spinnerCustomText"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <EditText
                android:id="@+id/txtEnglish"
                android:layout_width="600dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/bg_edittext"
                android:ems="10"
                android:inputType="textMultiLine"
                android:padding="5dp"
                android:singleLine="false"
                android:text="Printer testing program!\nTake it easy!"
                android:textColor="#333"
                android:textSize="14sp" />

            <Button
                android:id="@+id/btnEnglish"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@drawable/common_button"
                android:text="@string/Print"
                android:textSize="@dimen/common_button_text_size"
                android:textColor="@color/white" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                android:text="@string/Barcode"
                android:textSize="18sp"
                android:textColor="#888"  />

            <EditText
                android:id="@+id/txtBarcode"
                android:layout_width="600dp"
                android:layout_height="60dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/bg_edittext"
                android:ems="10"
                android:inputType="textMultiLine"
                android:padding="5dp"
                android:singleLine="false"
                android:text="0123456789"
                android:textColor="#333"
                android:textSize="14sp" />

            <Button
                android:id="@+id/btnBarcode"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@drawable/common_button"
                android:text="@string/Print"
                android:textSize="@dimen/common_button_text_size"
                android:textColor="@color/white" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btnDownloadNVImage"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@drawable/common_button"
                android:text="Download"
                android:textSize="@dimen/common_button_text_size"
                android:textColor="@color/white" />
            <Button
                android:id="@+id/btnPrintNVImage"
                android:layout_width="110dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/common_button"
                android:text="Print"
                android:textSize="@dimen/common_button_text_size"
                android:textColor="@color/white" />
        </LinearLayout>
        <Button
            android:id="@+id/btnRastPrint"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rast Print"
            android:visibility="invisible"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:background="@drawable/common_button"
            android:textSize="@dimen/common_button_text_size"/>
    </LinearLayout>


</LinearLayout>